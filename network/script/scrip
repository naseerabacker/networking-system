#read 3 numbers and find largest
echo "enter three numbers"
read n1 n2 n3
if [ $n1 -gt $n2 ]
then
max=$n1
else
max=$n2
fi
if [ $n3 -gt $max ]
then
max=$n3
fi
echo "largest is $max"
exit 0

/* output*/

$ vi max3.sh
$ sh max3.sh
enter three numbers
3 5 1 
largest is 5
$ sh max3.sh
enter three numbers
6576 54 768
largest is 6576

                                                                             
~       
